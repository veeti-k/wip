{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Node 16",
	"extends": "./base.json",
	"compilerOptions": {
		"module": "NodeNext",
		"target": "ES2020",
		"moduleResolution": "NodeNext",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	}
}
